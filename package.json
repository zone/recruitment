{
  "name": "zone-tech",
  "private": true,
  "version": "0.0.0",
  "author": "Mike Simmonds (https://simmo.codes)",
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@zonedigital/browserslist-config": "^3.1.1",
    "@zonedigital/eslint-config-react": "^3.1.0",
    "emotion": "^10.0.5",
    "emotion-server": "^10.0.5",
    "eslint": "^5.11.1",
    "eslint-plugin-compat": "^2.6.3",
    "gatsby": "^2.0.53",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-emotion": "^4.0.1",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-remark": "^2.1.15",
    "gatsby-transformer-sharp": "^2.1.8",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "gatsby build && gh-pages -d public -b master"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "prettier": "^1.15.2"
  },
  "repository": "git@github.com:zone/zone.github.io.git",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "@zonedigital/eslint-config-react"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "allowImportExportEverywhere": true
      }
    },
    "rules": {
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "browserslist": [
    "extends @zonedigital/browserslist-config"
  ]
}
